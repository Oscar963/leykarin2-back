{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "barryvdh/laravel-dompdf": "^2.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.23",
        "laravel/tinker": "^2.5",
        "laraveles/spanish": "^1.5",
        "maatwebsite/excel": "^3.1",
        "phpoffice/phpword": "^1.4",
        "socialiteproviders/claveunica": "^4.1",
        "spatie/laravel-permission": "^5.11",
        "stevebauman/location": "^6.6"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.7",
        "darkaonline/l5-swagger": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "fresh": [
            "@php artisan migrate:fresh --seed",
            "@php artisan cache:clear",
            "@php artisan config:clear",
            "@php artisan route:clear",
            "@php artisan view:clear"
        ],
        "setup": [
            "composer install",
            "cp .env.example .env",
            "@php artisan key:generate",
            "@php artisan migrate:fresh --seed",
            "@php artisan storage:link",
            "@php artisan l5-swagger:generate"
        ],
        "test": [
            "@php artisan test --parallel"
        ],
        "test-coverage": [
            "@php artisan test --coverage --min=80"
        ],
        "analyze": [
            "vendor\\bin\\phpcs --standard=PSR12 app/",
            "@php artisan route:list --columns=uri,method,middleware"
        ],
        "fix": [
            "vendor\\bin\\phpcbf --standard=PSR12 app/"
        ],
        "security": [
            "@php artisan route:list --columns=uri,method,middleware",
            "@php artisan permissions:check-unused",
            "@php artisan activity-log:clear --days=90"
        ],
        "optimize": [
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan view:cache",
            "@php artisan event:cache"
        ],
        "deploy": [
            "@optimize",
            "@php artisan migrate --force",
            "@php artisan queue:restart"
        ],
        "docs": [
            "@php artisan l5-swagger:generate",
            "@php artisan route:list --columns=uri,method,name > routes.txt"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
